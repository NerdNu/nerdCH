nerdch.level.admin:/disrandomblock = >>>
        @blocks = array_filter(all_materials(), closure(@key, @value){
                return(material_info(@value)[isBlock])
        });
        run( '/dis falling_block ' . @blocks[rand(array_size(@blocks))] . ' setGridLocked true setInvisible true')
<<<

nerdch.level.admin:/disrandomitem = >>>
        run( '/dis dropped_item ' . all_materials()[rand(array_size(all_materials()))] . ' setInvisible true')
<<<

nerdch.level.admin:/disrandomplayer = >>>
        array @players = all_players()
        run( '/dis player ' . @players[rand(array_size(@players))])
<<<
