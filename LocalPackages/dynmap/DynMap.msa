*:/dynmap = >>>
	if (player() == '~console' || array_contains(pgroup(), 'super')) {
		run('/dynmap');
	} else {
		msg(color(RED).'Usage: /dynmap [show | hide]');
	}
<<<

*:/dynmap $ = >>>
	if (player() == '~console' || array_contains(pgroup(), 'super') || $ == 'show' || $ == 'hide') {
		run('/dynmap '.$);
	} else {
		die(color(RED).'Unknown command. Type /help for help.');
	}
<<<

*:/dmarker [$] = >>>
	if (player() == '~console' || array_contains(pgroup(), 'super')) {
