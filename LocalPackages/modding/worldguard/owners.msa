*:/owners $region = >>>
	@owners = sk_region_info($region, pworld())[1]['players'];
	@sep = '';
	@list = '';
	foreach (@player in @owners) {
		try(
			@list = @list . @sep . plast_known_name(@player);
			@sep = ', ';
		);
	}
	msg(color('GRAY') . 'Owners of ' . $region . ': ' . color('WHITE') . @list);
<<<
