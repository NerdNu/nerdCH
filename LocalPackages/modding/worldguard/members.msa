*:/members $region = >>>
	@info = sk_region_info($region, pworld());
	@members = array_merge(@info[1]['players'], @info[2]['players']);
	@sep = '';
	@list = '';
	foreach (@player in @members) {
		try(
			@list = @list . @sep . plast_known_name(@player);
			@sep = ', ';
		);
	}
	msg(color('GRAY') . 'Members of ' . $region . ': ' . color('WHITE') . @list);
<<<
